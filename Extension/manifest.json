{
    "name": "Quotes From Dal",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "Random quotes from the @QuotesFromDal twitter page",
    "icons": {
        "16": "images/icon16.png",
        "128": "images/icon128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/chromereload.js",
            "scripts/background.js"
        ]
    },
  "browser_action": {
    "default_title": "Random quotes",
    "default_popup": "popup.html"
  },
  "permissions": ["http://127.0.0.1:9393/"]
}
